<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sherryyue.trntlgbt" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>不明小站</name>
    <description>
        不明小站手機版
    </description>
    <author email="sherryyue.c@protonmail.com" href="https://sherryyuechiu.github.io/card">
        SherryYue玥餅
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="res/logo.png"/>
</widget>
